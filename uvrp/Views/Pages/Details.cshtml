@model uvrp.Page

@{
    ViewBag.Title = "Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="banner inner-page-banner" style="background: url('/Content/img/@Model.HeaderImage') no-repeat center;">
   <div class="layer"></div>
    <div class="content">
        <h1>@Html.DisplayFor(model => model.Title)</h1>
        <div class="text-left">
            @Html.Raw(Model.Subtitle)
        </div>
    </div>
</div>
<div class="common-section">
    <div class="container">
        <div class="section-heading">
            <h2>@Html.DisplayFor(model => model.Title)</h2>
        </div>
        
        <div class="text-left">
            @Html.Raw(Model.Content)
        </div>
    </div>
</div>
