@* @model TModel *@

@{
    ViewBag.Title = "Page Hierarchy";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h2>Page hierarchy</h2>
<input type="hidden" id="addClass" value="1" />
<div id="tree1"></div>

<script type="text/javascript" >

    var data = [
        {
            name: 'node1',
            children: [
                { name: 'child1' },
                { name: 'child2' }
            ]
        },
        {
            name: 'node2',
            children: [
                { name: 'child3' }
            ]
        }
    ];
    
    $(function() {
        $('#tree1').tree({
            data: data
        });
    });

</script>